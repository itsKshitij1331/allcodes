void setup()
{
Serial.begin(9600);
pinMode(LED_BULTIN,OUTPUT);
}
void loop()
{
Serial.println(analogRead(A0));
if(analogRead(A0)>100)
{
digitalWrite(13,HIGH);
}
ELSE{
digitalWrite(13,LOW);
}
