LED-cathode(gnd),anode-13
tmp:-left-5v,middle-a0,right-gnd



/////////////////////////
void setup()
{
pinMode(13,OUTPUT);
pinMode(3,INPUT);
}
void loop()
{
if(digitalRead(3)==1)
{
digitalWrite(13,HIGH);
}
else{
digitalWrite(13,LOW);
}
}
switch slider
/////////////////////////////
const int gas=A0;
const int LED=13;
void setup()
{
  pinMode(gas,INPUT);
  pinMode(LED,OUTPUT);
}
 void loop()
 {
   if(analogRead(A0)>250)
   {
     digitalWrite(13,HIGH);
     delay(10);
   }
   else{
     digitalWrite(13,LOW);
     delay(10);
   }
 }

