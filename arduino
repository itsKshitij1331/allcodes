void setup()
{
Serial.begin(9600);
pinMode(LED_BULTIN,OUTPUT);
}
void loop()
{
Serial.println(analogRead(A0));
if(analogRead(A0))>101)
{
digitalWrite(LED_BUILTIN,HIGH);
}
else{
digitalWrite(ledbuiltin,low);
}

